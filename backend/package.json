{
  "name": "cipherstudio",
  "version": "1.0.0",
  "description": "CipherStudio - Browser-based React IDE with live preview and project persistence",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"npm run start-client\"",
    "start-client": "cd client && npm start",
    "build": "cd client && npm run build"
  },
